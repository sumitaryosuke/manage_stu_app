<div class="main_body">
  <div class>
  <h2>お知らせ</h2>
  </div>
  <div class="events">
    <div class="event-content">
      <ul class="event-list">
        <% @events.each do |eve| %>
          <li class="list">
            <%= link_to event_path(eve.id), method: :get do %>
              <div class="list-wrapper">
                <div><h3>
                <%= eve.name %>
                </h3></div>
                <div>
                <%= eve.start_day %> から
                </div>
                <div>
                <%= eve.end_day %> まで
                </div>
              </div>
            <% end %>
          </li>
        <% end %>
      </ul>
    </div>
  </div>
</div>